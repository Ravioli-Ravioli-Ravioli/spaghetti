<form class="form-inline" action="/incomepage" method="POST">
  <div class="form-group">
    <label for="query">QUERY</label>
    <input
      type="name"
      id="query"
      name="query"
      class="form-control"
      placeholder=""
      value="<%= typeof name != 'undefined' ? name : '' %>"
    />
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>

<br>
<table class="table table-striped table-bordered">
    <thead>
      <tr>
        <th width="7%" class="align-middle"><b>BILLING DATE</b></th>
        <th width="15%" class="align-middle"><b>CLIENT NAME</b></th>
        <th width="8%" class="align-middle"><b>BILL NO.</b></th>
        <th width="22%" class="align-middle"><b>AGENCY</b></th>
        <th width="26%" class="align-middle"><b>SERVICE TYPE</b></th>
        <th width="7%" class="align-middle"><b>AMOUNT</b></th>
        <th width="5%" class="align-middle"><b>OR NO.</b></th>
        <th width="8%" class="align-middle"><b>PAYMENT DATE</b></th>
        <th width="7%" class="align-middle"><b>COLLECTED</b></th>
        <th width="7%" class="align-middle"><b>UNCOLLECTED</b></th>
      </tr>
    </thead>
</table>
<div class="tableFixHead">
<table class="table table-striped table-bordered">
    <% incomelist.forEach(function(item) { %>
      <tr>
        <td width="5%" class="align-middle"><%= item.billDate %></td>
        <td width="15%" class="align-middle"><%= item.clientName %></td>
        <td width="7%" class="align-middle"><%= item.billNum %></td>
        <td width="20%" class="align-middle"><%= item.agency %></td>
        <td width="25%" class="align-middle"><%= item.type %></td>
        <td width="7%" class="align-middle"><%= item.amount %></td>
        <td idth="7%" class="align-middle"><%= item.ORNumber %></td>
        <td width="7%" class="align-middle"><%= item.paymentDate %></td>
        <td width="7%" class="align-middle"><%= item.collected %></td>
        <td width="7%" class="align-middle"><%= item.uncollected %></td>
      </tr>
    <% }); %>
</table>
</div>
