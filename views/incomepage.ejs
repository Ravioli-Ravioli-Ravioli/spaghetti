<h1 class="mt-4">Dashboard</h1>
<a href="/users/logout" class="btn btn-secondary">Logout</a>
<br>

<br>
<h2>CFB Fund Status</h2>
<br>
<table id="dtVerticalScrollExample" class="table table-striped table-bordered table-sm" cellspacing="0"
  width="100%">
    <thead>
      <tr>
        <th class="align-middle"><b>BILLING DATE</b></th>
        <th class="align-middle"><b>CLIENT NAME</b></th>
        <th class="align-middle"><b>BILL NO.</b></th>
        <th class="align-middle"><b>AGENCY</b></th>
        <th class="align-middle"><b>SERVICE TYPE</b></th>
        <th class="align-middle"><b>AMOUNT</b></th>
        <th class="align-middle"><b>OR NO.</b></th>
        <th class="align-middle"><b>PAYMENT DATE</b></th>
        <th class="align-middle"><b>COLLECTED</b></th>
        <th class="align-middle"><b>UNCOLLECTED</b></th>
      </tr>
    </thead>

    <% incomelist.forEach(function(item) { %>
      <tr>
        <th class="align-middle"><%= item.billDate %></th>
        <th width="15%" class="align-middle"><%= item.clientName %></th>
        <th class="align-middle"><%= item.billNum %></th>
        <th class="align-middle"><%= item.agency %></th>
        <th class="align-middle"><%= item.type %></th>
        <th class="align-middle"><%= item.amount %></th>
        <th class="align-middle"><%= item.ORNumber %></th>
        <th width="10%" class="align-middle"><%= item.paymentDate %></th>
        <th class="align-middle"><%= item.collected %></th>
        <th class="align-middle"><%= item.uncollected %></th>
      </tr>
    <% }); %>
    <tfoot>
        <tr>
          <th class="th-sm">Billing Date</th>
          <th class="th-sm">Client Name</th>
          <th class="th-sm">Bill Number</th>
          <th class="th-sm">Agency</th>
          <th class="th-sm">Service Type</th>
          <th class="th-sm">Amount</th>
          <th class="th-sm">OR Number</th>
          <th class="th-sm">Payment Date</th>
          <th class="th-sm">Collected</th>
          <th class="th-sm">Uncollected</th>
        </tr>
      </tfoot>
</table>
